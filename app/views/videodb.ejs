</a>
  <title>Video List</title>
</head>
<body>
  <div class="text-center" ng-controller="AppCtrl">
    <h1>Video List</h1>

  <table class="table">
      <thead>
        <tr>
          <th>File Name</th> 
          <th>&nbsp;</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><input class="form-control" ng-model="gridfs.filename"></td>
          <td><button class="btn btn-primary" ng-click="addgridfs()">Add Videos</button></td>
          <td><button class="btn btn-info" ng-click="update()">Update</button>&nbsp;&nbsp;<button class="btn btn-info" ng-click="deselect()">Clear</button></td>
        </tr>
        <tr ng-repeat="gridfs in fs.files">
          <td>{{gridfs.filename}}</td>
		<td><button class="btn btn-danger" ng-click="remove(gridfs._id)">Remove</button></td>
          <td><button class="btn btn-warning" ng-click="edit(gridfs._id)">Edit</button></td>
        </tr>
      </tbody>
    </table>

  </div>
    </div>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.12/angular.min.js"></script>
<script src="controllers/controller.js"></script>
