<body ng-app="myApp">
  <div class="text-center" ng-controller="AppCtrl">
    <h1>Ebooks List</h1>

  <table class="table">
      <thead>
        <tr>
          <th>File Name</th> 
          <th>&nbsp;</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><input class="form-control" ng-model="ebookgridfs.filename"></td>
          <td><button class="btn btn-primary" ng-click="addebookgridfs()">Add Videos</button></td>
          <td><button class="btn btn-info" ng-click="update()">Update</button>&nbsp;&nbsp;<button class="btn btn-info" ng-click="deselect()">Clear</button></td>
        </tr>
        <tr ng-repeat="ebookgridfs in fs.files">
          <td>{{ebookgridfs.filename}}</td>
		<td><button class="btn btn-danger" ng-click="remove(ebookgridfs._id)">Remove</button></td>
          <td><button class="btn btn-warning" ng-click="edit(ebookgridfs._id)">Edit</button></td>
        </tr>
      </tbody>
    </table>

  </div>
